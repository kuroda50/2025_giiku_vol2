{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "This is a sample Google Chrome extension.",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
    }
  }